pipeline {
   stages {
       stage('Back-end') {
           agent {
               agent slave-jenkins-gpu-01
               docker { image 'maven:3-alpine' }
           }
           steps {
               sh 'mvn --version'
           }
       }
       stage('Front-end') {
           agent {
               agent slave-jenkins-gpu-01
               docker { image 'node:7-alpine' }
           }
           steps {
               sh 'node --version'
           }
       }
   }
}